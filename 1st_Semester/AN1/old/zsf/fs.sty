\newtcbox{\mathhighlight}[1][]{%
    nobeforeafter, 
    math upper, 
    tcbox raise base,
    enhanced, 
    colback=mathHighlightColor!60, 
    boxrule=0pt,
    left=1mm,
    right=1mm,
    top=0.5mm,
    bottom=0.5mm,
    #1}

\newtcbox{\inlineieq}[1][]{%
    nobeforeafter, 
    math upper, 
    tcbox raise base,
    enhanced,  
    colback=mathHighlightColor!60, 
    boxrule=0pt,
    arc=3pt, 
    left=0mm,
    right=0mm,
    top=0mm,
    bottom=0mm,
    #1}


\newenvironment{iequation}[1][equation*]{%
    \empheq[box={\mathhighlight}]{#1}}{\endempheq}


